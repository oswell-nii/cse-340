

<main>
    <h1>Welcome <%= accountData.account_firstname %>!</h1>
    <% if (message) { %>
      <div class="flash-message"><%= message %></div>
    <% } %>
    <% if (errors && errors.length > 0) { %>
      <ul class="errors">
        <% errors.forEach(function(error) { %>
          <li><%= error %></li>
        <% }) %>
      </ul>
    <% } %>
    <% if (loggedin) { %>
      <p>
        <a href="/account/edit" class="btn-edit-account">
          <i class="fas fa-user-edit"></i> Edit Account Information
        </a>
      </p>
    <% } %>
    
    
</main>